
#--- Alarm summaries ------------------------------
record(calc, "$(P):DiskAlarmSum1") {
    field(INPA, "$(P):Disk1Use CPP MS")
    field(INPB, "$(P):Disk2Use CPP MS")
    field(INPC, "$(P):Disk3Use CPP MS")
    field(INPD, "$(P):Disk4Use CPP MS")
    field(INPE, "$(P):Disk5Use CPP MS")
    field(INPF, "$(P):Disk6Use CPP MS")
    field(INPG, "$(P):Disk7Use CPP MS")
    field(INPH, "$(P):Disk8Use CPP MS")
    field(INPI, "$(P):Disk9Use CPP MS")
    field(INPJ, "$(P):Disk10Use CPP MS")
    field(INPK, "$(P):Disk11Use CPP MS")
    field(INPL, "$(P):Disk12Use CPP MS")
    field(PINI, "YES")
    field(FLNK, "$(P):DiskAlarmSum")
}

