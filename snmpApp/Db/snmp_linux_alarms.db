record(calc, "$(P):DiskAlarmSum") {
    field(INPA, "$(P):DiskAlarmSum1 NPP MS")
    field(INPB, "$(P):DiskAlarmSum2 NPP MS")
    field(INPC, "$(P):DiskAlarmSum3 NPP MS")
    field(INPD, "$(P):DiskAlarmSum4 NPP MS")
