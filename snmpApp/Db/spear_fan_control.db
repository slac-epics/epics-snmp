
record(bo, "$(P):FansAllOn") {
    field(DESC, "All spear fans on")
    field(ZNAM, "$(ZNAM="Off")")
    field(ONAM, "$(ONAM="On")")
    field(ZSV,  "$(ZSV=NO_ALARM)")
    field(OSV,  "$(OSV=NO_ALARM)")
    field(FLNK, "$(P):FansAllOnCalc")
}

record(calc, "$(P):FansAllOnCalc") {
    field(DESC, "All spear fans on")
    field(INPA, "$(P):FansAllOn")
    field(CALC, "A?2:1")
    field(FLNK, "$(P):FansAllOnSeq")
}

